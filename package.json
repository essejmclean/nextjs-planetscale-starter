{
  "name": "starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "yarn -s prisma:generate && yarn -s nexus:generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "nexus:generate": "ts-node --transpile-only -P nexus.tsconfig.json graphql/schema",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:reset": "prisma migrate reset --skip-seed",
    "prisma:push": "prisma db push --skip-seed",
    "prisma:seed": "npx prisma db seed",
    "prisma:studio": "prisma studio",
    "server:main": "pscale connect netlify-starter main --port 3309",
    "server:shadow": "pscale connect netlify-starter shadow --port 3310"
  },
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "@next-auth/prisma-adapter": "^0.5.2-next.19",
    "@prisma/client": "^3.5.0",
    "@tailwindcss/forms": "^0.2.1",
    "apollo-server-micro": "^3.5.0",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "graphql": "15.7.2",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "^12.0.2",
    "next-auth": "4.0.0-beta.7",
    "next-connect": "^0.10.2",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.18.1",
    "react-query": "^3.23.2",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.0.0-beta.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.176",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/superagent": "^4.1.13",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "mockdate": "^3.0.5",
    "postcss": "^8.2.8",
    "prettier": "^2.3.1",
    "prisma": "^3.5.0",
    "tailwindcss": "^2.2.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
